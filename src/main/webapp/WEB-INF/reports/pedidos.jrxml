	<queryString>
		<![CDATA[SELECT
     pedido.`ped_id` AS pedido_ped_id,
     pedido.`ped_data` AS pedido_ped_data,
     pedido.`ped_descricao` AS pedido_ped_descricao,
     pedido.`ped_detalhes` AS pedido_ped_detalhes,
     pedido.`ped_cliente` AS pedido_ped_cliente,
     pedido.`ped_funcionario` AS pedido_ped_funcionario,
     cliente.`cli_data_cadastro` AS cliente_cli_data_cadastro,
     cliente.`cli_nome_razao` AS cliente_cli_nome_razao,
     cliente.`cli_tipo_pessoa` AS cliente_cli_tipo_pessoa,
     cliente.`pes_id` AS cliente_pes_id
FROM
     `cliente` cliente INNER JOIN `pedido` pedido ON cliente.`pes_id` = pedido.`ped_cliente`
WHERE
     cliente.`cli_nome_razao` LIKE '%$P!{parametros}%'
ORDER BY
     cliente_cli_nome_razao ASC]]>
